{
  "name": "@goldenius/hades-js",
  "version": "1.0.10",
  "description": "Calculate positions of the planets from an geocentric perspective, ASC, MC, houses and aspects providing date, time, and coordinates.",
  "main": "./distribution/index.js",
  "scripts": {
    "test": "npm run prepublish && npm run copyResources && npm run dev",
    "dev": "node ./tests/initializationTests.js",
    "build": "babel source --presets babel-preset-env --out-dir distribution",
    "prepublish": "npm run build",
    "copyResources": "cp -r ./source/resources  ./distribution/resources"
  },
  "keywords": [
    "astrology",
    "placidus",
    "ascendant",
    "planetary-aspects"
  ],
  "author": "Brian Langer <brian@supracoelum.com> (http://www.supracoelum.com)",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "meeusjs": "^1.0.4",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.21",
    "moment-timezone-all": "^0.5.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0"
  }
}
